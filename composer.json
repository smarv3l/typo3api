{
    "name": "nemo64/typo3api",
    "type": "typo3-cms-extension",
    "license": "MIT",
    "keywords": [
        "TYPO3",
        "api",
        "TCA",
        "tsconfig",
        "ext_tables.php",
        "ext_localconf.php"
    ],
    "support": {
        "issues": "https://github.com/Nemo64/typo3api/issues"
    },
    "authors": [
        {
            "name": "Marco Pfeiffer",
            "role": "Developer",
            "homepage": "https://www.marco.zone/"
        }
    ],
    "extra": {
        "typo3/cms": {
            "web-dir": "web"
        }
    },
    "require": {
        "php": "^7.0",
        "symfony/intl": ">= 2.3, < 5.0",
        "symfony/options-resolver": ">= 2.6, < 5.3",
        "symfony/polyfill-mbstring": "^1.2",
        "typo3/cms-backend": "~8.7.8 || ~9.5.0 || ~10.4.0",
        "typo3/cms-core": "~8.7.8 || ~9.5.0 || ~10.4.0",
        "typo3/cms-extbase": "~8.7.8 || ~9.5.0 || ~10.4.0",
        "typo3/cms-frontend": "~8.7.8 || ~9.5.0 || ~10.4.0",
        "typo3/cms-install": "~8.7.8 || ~9.5.0 || ~10.4.0",
        "typo3/cms-lang": "~8.7.8 || ~9.5.0 || ~10.4.0"
    },
    "autoload": {
        "psr-4": {
            "Typo3Api\\": "Classes"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "typo3/cms-composer-installers": ">=1.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Typo3Api\\": "Tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
